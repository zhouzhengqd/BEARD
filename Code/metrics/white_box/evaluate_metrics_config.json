{
    "input_files": [
        "./BACON/ConvNet_CIFAR10_1_evaluation_log.txt",
        "./BACON/ConvNet_CIFAR10_10_evaluation_log.txt",
        "./BACON/ConvNet_CIFAR10_50_evaluation_log.txt"
    ],
    "output_dir": "./BACON/",
    "metrics_type": "beard",
    "alpha": 0.5,
    "dataset": "CIFAR10",
    "method": "BACON",
